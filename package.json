{
  "name": "axon",
  "private": true,
  "version": "2.4.0",
  "type": "module",
  "homepage": "https://axon-app.github.io/Axon.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/axon-app/Axon.app.git"
  },
  "scripts": {
    "dev": "vite",
    "dev:alt": "vite --port 3001",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "clean": "rimraf dist",
    "clean:modules": "rimraf node_modules",
    "clean:all": "rimraf dist node_modules package-lock.json",
    "clean:build": "npm run clean && npm run build",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy:production": "npm run clean:build && gh-pages -d dist",
    "analyze": "rollup-plugin-visualizer"
  },
  "dependencies": {
    "@fontsource/orbitron": "^5.2.6",
    "@fontsource/rajdhani": "^5.2.6",
    "canvas": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.27.5",
    "@babel/template": "^7.27.2",
    "@babel/traverse": "^7.27.4",
    "@eslint/js": "^9.25.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^24.0.3",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "postcss": "^8.4.31",
    "prop-types": "^15.8.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^4.1.10",
    "terser": "^5.42.0",
    "to-ico": "^1.0.1",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
